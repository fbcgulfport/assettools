		"@types/react": "19.2.2",
		"@types/react-dom": "19.2.2",
		"drizzle-kit": "0.31.5",
		"react-email": "4.3.0",
		"typescript": "5.9.3"
	},
	"dependencies": {
		"@react-email/components": "0.5.6",
		"@react-email/render": "1.3.2",
		"@react-email/tailwind": "^1.2.2",
		"drizzle-orm": "0.44.6",
		"nodemailer": "7.0.9",
		"react": "19.2.0",
		"react-dom": "19.2.0"
